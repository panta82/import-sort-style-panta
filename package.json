{
  "name": "import-sort-style-panta",
  "version": "0.0.1",
  "description": "A style for \"import-sort\". Based on \"import-sort-style-module\", with additional rules for CSS and images.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "test": "jest",
    "watch": "jest --watchAll",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "import-sort": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^27.5.1",
    "prettier": "latest",
    "prettier-plugin-import-sort": "0.0.7",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "keywords": [
    "import-sort",
    "import-sort-style",
    "import-sort-style-module",
    "prettier",
    "sort",
    "eslint"
  ],
  "author": "Ivan Pantic",
  "license": "MIT",
  "importSort": {
    ".ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    },
    ".js, .jsx": {
      "style": "module"
    }
  }
}
